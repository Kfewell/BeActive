<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="wrap_content"
  android:layout_height="match_parent">
   
    <LinearLayout android:layout_width="wrap_content" android:id="@+id/linearLayout1" android:layout_height="wrap_content" android:orientation="vertical">
    	 <TextView android:id="@+id/tunerTextView" android:layout_width="wrap_content" android:textAppearance="?android:attr/textAppearanceLarge"  android:padding="20dip" android:layout_height="wrap_content" android:text="-"></TextView>
	    <SeekBar android:layout_width="245dp" android:layout_height="wrap_content" android:id="@+id/tunerSeekBar" android:max="200"  android:padding="20dip"></SeekBar>
	    <!--  8-15 range --> 
	        <Button android:text="Done" android:id="@+id/tunerButton" android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
	     
    </LinearLayout>
</LinearLayout>
